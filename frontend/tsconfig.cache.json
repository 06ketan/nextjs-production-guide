{
  "compilerOptions": {
    "outDir": ".cache-handler",
    "noEmit": false,
    "declaration": false,
    "module": "CommonJS",
    "target": "ES2020",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": false,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["src/lib/cache/**/*.ts"],
  "exclude": ["node_modules"]
}
